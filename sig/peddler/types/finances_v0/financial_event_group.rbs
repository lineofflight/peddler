module Peddler
  module Types
    module FinancesV0
      class FinancialEventGroup < Data
        attr_reader financial_event_group_id: String?
        attr_reader processing_status: String?
        attr_reader fund_transfer_status: String?
        attr_reader original_total: Currency?
        attr_reader converted_total: Currency?
        attr_reader fund_transfer_date: String?
        attr_reader trace_id: String?
        attr_reader account_tail: String?
        attr_reader beginning_balance: Currency?
        attr_reader financial_event_group_start: String?
        attr_reader financial_event_group_end: String?

        def self.parse: (Hash[String, untyped] hash) -> instance

        def initialize: (
          ?financial_event_group_id: String,
          ?processing_status: String,
          ?fund_transfer_status: String,
          ?original_total: Currency,
          ?converted_total: Currency,
          ?fund_transfer_date: String,
          ?trace_id: String,
          ?account_tail: String,
          ?beginning_balance: Currency,
          ?financial_event_group_start: String,
          ?financial_event_group_end: String
