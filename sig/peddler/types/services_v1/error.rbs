module Peddler
  module Types
    module ServicesV1
      class Error < Data
        attr_reader code: String
        attr_reader message: String
        attr_reader details: String?
        attr_reader error_level: String?

        def self.parse: (Hash[String, untyped] hash) -> instance

        def initialize: (
          code: String,
          message: String,
          ?details: String,
          ?error_level: String
        ) -> void
      end
    end
  end
end
