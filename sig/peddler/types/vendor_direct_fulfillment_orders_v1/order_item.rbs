module Peddler
  module Types
    module VendorDirectFulfillmentOrdersV1
      class OrderItem < Data
        attr_reader item_sequence_number: String
        attr_reader buyer_product_identifier: String?
        attr_reader vendor_product_identifier: String?
        attr_reader title: String?
        attr_reader ordered_quantity: ItemQuantity
        attr_reader scheduled_delivery_shipment: ScheduledDeliveryShipment?
        attr_reader gift_details: GiftDetails?
        attr_reader net_price: Money
        attr_reader tax_details: Hash[String, untyped]?
        attr_reader total_price: Money?

        def self.parse: (Hash[String, untyped] hash) -> instance

        def initialize: (
          item_sequence_number: String,
          ?buyer_product_identifier: String,
          ?vendor_product_identifier: String,
          ?title: String,
          ordered_quantity: ItemQuantity,
          ?scheduled_delivery_shipment: ScheduledDeliveryShipment,
          ?gift_details: GiftDetails,
          net_price: Money,
          ?tax_details: Hash[String, untyped],
          ?total_price: Money
        ) -> void
      end
    end
  end
end
