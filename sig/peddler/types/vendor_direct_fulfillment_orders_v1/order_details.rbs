module Peddler
  module Types
    module VendorDirectFulfillmentOrdersV1
      class OrderDetails < Data
        attr_reader customer_order_number: String
        attr_reader order_date: String
        attr_reader order_status: String?
        attr_reader shipment_details: ShipmentDetails
        attr_reader tax_total: Hash[String, untyped]?
        attr_reader selling_party: PartyIdentification
        attr_reader ship_from_party: PartyIdentification
        attr_reader ship_to_party: Address
        attr_reader bill_to_party: PartyIdentification
        attr_reader items: Array[OrderItem]

        def self.parse: (Hash[String, untyped] hash) -> instance

        def initialize: (
          customer_order_number: String,
          order_date: String,
          ?order_status: String,
          shipment_details: ShipmentDetails,
          ?tax_total: Hash[String, untyped],
          selling_party: PartyIdentification,
          ship_from_party: PartyIdentification,
          ship_to_party: Address,
          bill_to_party: PartyIdentification,
          items: Array[OrderItem]
        ) -> void
      end
    end
  end
end
